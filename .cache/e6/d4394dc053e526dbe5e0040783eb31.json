\n            </NavLink>\r\n          </Nav>\r\n          <ContainerPages>\r\n            <Paginas />\r\n          </ContainerPages>\r\n        </Container>\r\n      </BrowserRouter>\r\n    </>\r\n  );\r\n}\r\n\r\nfunction Paginas() {\r\n  const location = useLocation();\r\n  return (\r\n    <AnimatePresence>\r\n      <Switch location={location} key={location.key}>\r\n        <Route path=\"/pagina1\" component={Pagina1} />\r\n        <Route path=\"/pagina2\" component={Pagina2} />\r\n        <Route path=\"/pagina3\" component={Pagina3} />\r\n      </Switch>\r\n    </AnimatePresence>\r\n  );\r\n}\r\n\r\nconst slide = {\r\n  mostrar: {\r\n    x: 0,\r\n    transition: {\r\n      delay: 0.2,\r\n    },\r\n  },\r\n  esconder: {\r\n    x: \"-100%\",\r\n    opacity: 0,\r\n  },\r\n  inicial: {\r\n    x: \"100%\",\r\n  },\r\n};\r\n\r\nfunction Pagina1() {\r\n  return (\r\n    <PaginaCor\r\n      corDaPagina=\"steelblue\"\r\n      variants={slide}\r\n      animate=\"mostrar\"\r\n      initial=\"inicial\"\r\n      exit=\"esconder\"\r\n    />\r\n  );\r\n}\r\nfunction Pagina2() {\r\n  return (\r\n    <PaginaCor\r\n      corDaPagina=\"limegreen\"\r\n      variants={slide}\r\n      animate=\"mostrar\"\r\n      initial=\"inicial\"\r\n      exit=\"esconder\"\r\n    />\r\n  );\r\n}\r\nfunction Pagina3() {\r\n  return (\r\n    <PaginaCor\r\n      corDaPagina=\"gold\"\r\n      variants={slide}\r\n      animate=\"mostrar\"\r\n      initial=\"inicial\"\r\n      exit=\"esconder\"\r\n    />\r\n  );\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"32697ac35bf775ddada054ba075baaf7","cacheData":{"env":{}}}